<?xml version="1.0" ?>
<robot name="cube" xmlns:xacro="http://www.ros.org/wiki/xacro">
  <material name="black">
    <color rgba="0.0 0.0 0.0 1.0"/>
  </material>
  <material name="blue">
    <color rgba="0.203125 0.23828125 0.28515625 1.0"/>
  </material>
  <material name="green">
    <color rgba="0.0 0.8 0.0 1.0"/>
  </material>
  <material name="grey">
    <color rgba="0.2 0.2 0.2 1.0"/>
  </material>
  <material name="orange">
    <color rgba="1.0 0.423529411765 0.0392156862745 1.0"/>
  </material>
  <material name="brown">
    <color rgba="0.870588235294 0.811764705882 0.764705882353 1.0"/>
  </material>
  <material name="red">
    <color rgba="0.80078125 0.12890625 0.1328125 1.0"/>
  </material>
  <material name="white">
    <color rgba="1.0 1.0 1.0 1.0"/>
  </material>
  
  <gazebo reference="base">
    <material>Gazebo/White</material>
  </gazebo>
  <gazebo reference="first_link">
    <material>Gazebo/Blue</material>
  </gazebo>
  <gazebo reference="fourth_link">
    <material>Gazebo/Orange</material>
  </gazebo>
  
  <gazebo>
    <plugin filename="libgazebo_ros_diff_drive.so" name="differential_drive_controller">
      <legacyMode>false</legacyMode>
      <alwaysOn>true</alwaysOn>
      <updateRate>20</updateRate>
      <leftJoint>joint_left_wheel</leftJoint>
      <rightJoint>joint_right_wheel</rightJoint>
      <wheelSeparation>0.2</wheelSeparation>
      <wheelDiameter>0.2</wheelDiameter>
      <torque>0.1</torque>
      <commandTopic>cmd_vel</commandTopic>
      <odometryTopic>odom</odometryTopic>
      <odometryFrame>odom</odometryFrame>
      <robotBaseFrame>link_chassis</robotBaseFrame>
    </plugin>
  </gazebo>
  
  <link name="base">
    <!-- pose and inertial -->
    <pose>0 0 0 0 0 0</pose>
    <inertial>
      <mass value="5"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0.0395416666667" ixy="0" ixz="0" iyy="0.106208333333" iyz="0" izz="0.106208333333"/>
    </inertial>
    <!-- body -->
    <collision name="collision_chassis">
      <geometry>
        <box size="0.5 0.5 0.1"/>
      </geometry>
    </collision>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.5 0.5 0.1"/>
      </geometry>
      <material name="white"/>
    </visual>
  </link>


 <link name="first_link">
    <inertial>
      <mass value="0.2"/>
      <!-- origin rpy="0 1.5707 1.5707" xyz="0 0 0"-->
      <inertia ixx="0.000526666666667" ixy="0" ixz="0" iyy="0.000526666666667" iyz="0" izz="0.001"/>
    </inertial>
    <collision name="first_link_collision">
      <origin rpy="0 0 1.5707" xyz="0 0 .05"/>
      <geometry>
        <cylinder length="0.1" radius="0.2"/>
      </geometry>
    </collision>
    <visual name="first_link_visual">
      <origin rpy="0 0 0" xyz="0 0 .05"/>
      <geometry>
        <cylinder length="0.1" radius="0.2"/>
      </geometry>
	<material name="blue"/>
    </visual>
  </link>
  
<joint name="joint1" type="revolute">
    <parent link="base"/>
    <child link="first_link"/>
    <origin xyz="0 0 .05" rpy="0 0 0"/>
    <limit lower="-3.1416" upper="3.1416" effort="1000" velocity=".5"/>
    <axis xyz="0 0 1"/>
</joint>

<link name="second_link">
    <inertial>
      <mass value="0.2"/>
      <origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
      <inertia ixx="0.000526666666667" ixy="0" ixz="0" iyy="0.000526666666667" iyz="0" izz="0.001"/>
    </inertial>
    <collision name="second_link_collision">
      <origin rpy="0 0 1.5707" xyz="0 0 .1"/>
      <geometry>
        <cylinder length="0.5" radius="0.07"/>
      </geometry>
    </collision>
    <visual name="second_link_visual">
      <origin rpy="0 0 1.5707" xyz="0 0 .1"/>
      <geometry>
        <cylinder length="0.5" radius="0.07"/>
      </geometry>
	<material name="blue"/>
    </visual>
  </link>

<joint name="joint2" type="fixed">
    <parent link="first_link"/>
    <child link="second_link"/>
    <origin xyz="0 0 .25"/>
</joint>

 <link name="third_link">
    <inertial>
      <mass value="0.2"/>
      <origin rpy="0 0 1.5707" xyz="0 0 0"/>
      <inertia ixx="0.000526666666667" ixy="0" ixz="0" iyy="0.000526666666667" iyz="0" izz="0.001"/>
    </inertial>
    <collision name="second_link_collision">
      <origin rpy="0 0 1.5707" xyz="0 0 .25"/>
      <geometry>
        <cylinder length="0.5" radius="0.07"/>
      </geometry>
    </collision>
    <visual name="second_link_visual">
      <origin rpy="0 0 1.5707" xyz="0 0 .25"/>
      <geometry>
        <cylinder length="0.5" radius="0.07"/>
      </geometry>
	<material name="orange"/>
    </visual>
  </link>

 <joint name="joint3" type="revolute">
    <parent link="second_link"/>
    <child link="third_link"/>
    <origin xyz="0 0 .35" rpy="0 0 1.5707"/>
    <limit lower="-1.5707" upper="1.5707" effort="100" velocity="3"/>
    <axis xyz="0 1 0"/>
</joint>

<link name="fourth_link">
    <inertial>
      <mass value="0.2"/>
      <origin rpy="0 0 1.5707" xyz="0 0 0"/>
      <inertia ixx="0.000526666666667" ixy="0" ixz="0" iyy="0.000526666666667" iyz="0" izz="0.001"/>
    </inertial>
    <collision name="fourth_link_collision">
      <origin rpy="0 0 1.5707" xyz="0 0 .25"/>
      <geometry>
        <cylinder length="0.5" radius="0.07"/>
      </geometry>
    </collision>
    <visual name="fourth_link_visual">
      <origin rpy="0 0 1.5707" xyz="0 0 .25"/>
      <geometry>
        <cylinder length="0.5" radius="0.07"/>
      </geometry>
	<material name="orange"/>
    </visual>
  </link>

<joint name="joint4" type="revolute">
    <parent link="third_link"/>
    <child link="fourth_link"/>
    <origin xyz="0 0 .5" rpy="0 0 1.5707"/>
    <limit lower="-1.5707" upper="1.5707" effort="100" velocity="3"/>
    <axis xyz="1 0 0"/>
</joint>



 </robot>
